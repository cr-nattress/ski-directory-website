{
  "name": "migrate-regions",
  "version": "1.0.0",
  "description": "Migration tool for ski resort data to GCP Cloud Storage",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "ts-node src/index.ts",
    "migrate:state": "ts-node src/index.ts --state",
    "migrate:all": "ts-node src/index.ts --all",
    "dry-run": "ts-node src/index.ts --dry-run"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2"
  }
}
